{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "babel"
  ],
  "globals": {
    "alert": 1,
    "jest": 1,
    "describe": 1,
    "it": 1,
    "expect": 1,
    "beforeEach": 1,
    "document": 1,
    "window": 1,
    "goog_snippet_vars": 1,
    "goog_report_conversion": 1
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "space-before-function-paren": 0,
    "no-param-reassign": 0,
    "no-mixed-operators": 0,
    "no-return-await": 0,
    "react/prop-types": 0,
    "key-spacing": 0,
    "max-len": [
      2,
      120,
      2
    ],
    "object-curly-spacing": [
      2,
      "always"
    ],
    "class-methods-use-this": 0,
    "react/sort-comp": 0,
    "import/first": 0,
    "arrow-parens": 0
  }
}
